#include <stdio.h>
#include <stdlib.h>
#include <time.h>
/**
* main - Entry point
*
* Return: Always 0 (success)
*/
int main(void)
{
	int i;

	srand(time(NULL));

	i = rand() - (RAND_MAX / 2);
	printf("%d ", i);
	if (i < 0)
	{
		printf("is negative");
	}
	else if (i == 0)
	{
		printf("is zero");
	}
	else
	{
		printf("is positive");
	}
	printf("\n");
return (0);
}
